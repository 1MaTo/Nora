{
  "name": "nora",
  "version": "2.0.0",
  "description": "Typescript version",
  "main": "build/bot.js",
  "dependencies": {
    "@types/node": "^14.14.22",
    "axios": "^0.21.1",
    "core-js": "^3.8.1",
    "cross-env": "^7.0.3",
    "discord.js": "^12.5.1",
    "express": "^4.17.1",
    "fastify": "^3.10.1",
    "forever": "^3.0.4",
    "mysql": "^2.18.1",
    "redis": "^3.0.2",
    "shx": "^0.3.3",
    "slash-create": "^1.3.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {},
  "scripts": {
    "start": "tsc && cross-env NODE_ENV=development node build/bot.js",
    "clear_build": "shx rm -rf build/",
    "get_build": "git checkout build && git pull",
    "copy_auth": "shx touch build/auth.json && shx cp auth.json build/auth.json",
    "setup_build": "npm run clear_build && npm run get_build && npm run copy_auth",
    "prod_start": "cross-env NODE_ENV=production forever start build/bot.js",
    "production_bot_start": "npm run setup_build && npm run prod_start",
    "production_bot_stop": "forever stop build/bot.js",
    "production_bot_restart": "npm run setup_build && forever restart build/bot.js",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1MaTo/Nora.git"
  },
  "author": "MaTo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1MaTo/Nora/issues"
  },
  "homepage": "https://github.com/1MaTo/Nora#readme"
}
